{
    "name": "alm",
    "displayName": "alm",
    "description": "Use alm inside vscode",
    "version": "0.0.1",
    "publisher": "basarat",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.alm"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.alm",
            "title": "ALM"
        }]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}